for i in range(3):
    print("loop :",(i))
    for j in range(4):
        print(j)



start = int(input("start"))
end = int(input("end"))
for number in range(start, end + 1):
    print("Round:",number)
    print("-------------")
    for i in range(1, 12):
        print(number, "x",i, "=", number * i)
        
